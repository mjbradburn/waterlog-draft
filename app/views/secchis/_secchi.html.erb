    <tr data-toggle="collapse" data-target=".sample<%=secchi.id%>">
      <td><%= Lake.find(secchi.lake_id).lkname %> </td>
      <td><%= secchi.sample_time.to_s(:long) %></td>
      <td><%= secchi.secchi_depth_ft %></td> 

      <td><button type='button' class='btn btn-info' data-toggle='collapse' data-target=".sample<%=secchi.id%>">Details</td>
      <td><%= link_to 'Edit', edit_secchi_path(secchi), class: 'btn btn-mini btn btn-warning', remote: true %></td>
      <td><%= link_to 'Destroy', secchi, class: 'btn btn-mini btn btn-danger', method: :delete, data: { confirm: 'Are you sure?' }, remote: true%></td>
    </tr>
    <tr>
      <%=render 'show', :secchi => secchi %>
    </tr>